omizedSnackbar />\n\n        <Switch>\n          <Route exact path=\"/\" component={Home}></Route>\n\n          <Route exact path=\"/cart\" component={CartPage}></Route>\n          <Route exact path=\"/payment\" component={PaymentPage}></Route>\n          <Route exact path=\"/wishlist\" component={WishList}></Route>\n\n          <Route component={Page404} />\n        </Switch>\n\n        <Footer />\n      </Router>\n    </MuiThemeProvider>\n  );\n}\n\nfunction mapStateToProps(state) {\n  return {};\n}\n\nexport default connect(mapStateToProps)(App);\n","/home/droid_x/Documents/WEBD/ecomm-react/src/components/shared/Header/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/Home/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/shared/Footer/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/store/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/reducers/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/reducers/products.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/reducers/snackbar.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/actions/actionTypes.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/CartPage/index.js",["107","108"],"import React, { useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport { Paper } from '@material-ui/core';\n\nimport './cart.css';\nimport CartCard from './CartCard';\n\nfunction CartPage(props) {\n  let products = [];\n  let cartItems = props.cart.items;\n  let total = props.cart.total;\n\n  /*\n    Structure of items in store\n    items = {\n      '1231231ID':{\n        index: 2\n        price: 1000,\n        qty: 3,\n      }\n    }\n  */\n\n  if (products) {\n    Object.values(cartItems).map((item) => {\n      let tmp = props.products.productList[item.index];\n      tmp.qty = item.qty;\n      tmp.index = item.index;\n      if (item.qty) products.push(tmp);\n    });\n  }\n\n  return (\n    <div id=\"cart-page-container\">\n      <div id=\"cart-card-container\">\n        {products.map((product) => (\n          <CartCard product={product} />\n        ))}\n      </div>\n\n      <Paper id=\"cart-summary-container\">Total Amount: ₹{total}</Paper>\n    </div>\n  );\n}\n\nfunction mapStateToProps(state) {\n  return {\n    products: state.products,\n    cart: state.cart,\n  };\n}\n\nexport default connect(mapStateToProps)(CartPage);\n","/home/droid_x/Documents/WEBD/ecomm-react/src/components/PaymentPage/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/Page404.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/WishList/index.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/shared/SnackBar.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/actions/snackbar.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/actions/products.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/utils/urls.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/reducers/cart.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/components/Home/ProductCard.js",["109"],"import React, { useState } from 'react';\nimport { connect } from 'react-redux';\nimport {\n  Button,\n  Card,\n  CardActions,\n  CardContent,\n  CardHeader,\n  IconButton,\n  Typography,\n} from '@material-ui/core';\nimport FavoriteIcon from '@material-ui/icons/Favorite';\nimport FavoriteBorderIcon from '@material-ui/icons/FavoriteBorder';\nimport AddIcon from '@material-ui/icons/Add';\nimport RemoveIcon from '@material-ui/icons/Remove';\n\nimport { addToWishList, rmFromWishList } from '../../actions/products';\nimport { addToCart, rmFromCart } from '../../actions/cart';\n\nfunction ProductCard(props) {\n  let product = props.product;\n  let index = props.index;\n\n  // For WishList\n  let wishList = props.products.wishList;\n  let isInWishList = false;\n\n  for (let i = 0; wishList && i < wishList.length; i++) {\n    if (wishList[i]._id.$oid === product._id.$oid) {\n      isInWishList = true;\n      break;\n    }\n  }\n\n  let handleWishListToggle = () => {\n    isInWishList\n      ? props.dispatch(rmFromWishList(props.product))\n      : props.dispatch(addToWishList(props.product));\n  };\n\n  // for Cart\n  let items = props.cart.items;\n  let qty = 0;\n  if (items[product._id.$oid]) {\n    qty = items[product._id.$oid].qty;\n  }\n\n  let handleCartToggle = (type) => {\n    if (type === 'add') {\n      qty++;\n      props.dispatch(addToCart(product._id.$oid, product.price, index));\n    } else if (qty) {\n      qty--;\n      props.dispatch(rmFromCart(product._id.$oid, product.price, index));\n    }\n  };\n\n  return (\n    <Card className=\"home-card\" id={'prod-' + product._id.$oid}>\n      <img\n        src={product.images.large.url}\n        alt={product.label}\n        className=\"home card-img\"\n      />\n\n      <CardHeader\n        className=\"home-card-header\"\n        title={product.title}\n        subheader={product.brand}\n      />\n\n      <CardContent>\n        <Typography variant=\"body2\" color=\"textPrimary\">\n          ₹{product.price}\n        </Typography>\n        <Typography variant=\"body2\" color=\"textSecondary\">\n          {product.operatingsystem}\n        </Typography>\n      </CardContent>\n      <CardActions className=\"card-action-container\">\n        <IconButton onClick={handleWishListToggle}>\n          {isInWishList ? <FavoriteIcon /> : <FavoriteBorderIcon />}\n        </IconButton>\n        <div className=\"right-actions\">\n          {qty ? (\n            <React.Fragment>\n              <IconButton onClick={() => handleCartToggle('add')}>\n                <AddIcon />\n              </IconButton>\n              <Typography color=\"secondary\" display=\"inline\">\n                {qty}\n              </Typography>\n              <IconButton onClick={() => handleCartToggle('rm')}>\n                <RemoveIcon />\n              </IconButton>\n            </React.Fragment>\n          ) : (\n            <IconButton onClick={() => handleCartToggle('add')}>\n              <Button variant=\"outlined\" color=\"secondary\">\n                Add to Cart\n              </Button>\n            </IconButton>\n          )}\n        </div>\n      </CardActions>\n    </Card>\n  );\n}\n\nfunction mapStateToProps(state) {\n  return {\n    products: state.products,\n    cart: state.cart,\n  };\n}\n\nexport default connect(mapStateToProps)(ProductCard);\n","/home/droid_x/Documents/WEBD/ecomm-react/src/components/Home/Filters.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/utils/configs.js",[],"/home/droid_x/Documents/WEBD/ecomm-react/src/actions/cart.js",["110","111"],"import { ADD_TO_CART, RM_FROM_CART, CLEAR_CART } from './actionTypes';\n\nimport { setSnackBar } from './snackbar';\n\nexport function addToCart(id, price, index) {\n  return {\n    type: ADD_TO_CART,\n    id,\n    price,\n    index,\n  };\n}\n\nexport function rmFromCart(id, price, index) {\n  return {\n    type: RM_FROM_CART,\n    id,\n    price,\n    index,\n  };\n}\n","/home/droid_x/Documents/WEBD/ecomm-react/src/components/CartPage/CartCard.js",[],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":5,"column":3,"nodeType":"118","messageId":"119","endLine":5,"endColumn":11},{"ruleId":"120","severity":1,"message":"121","line":34,"column":6,"nodeType":"122","endLine":34,"endColumn":8,"suggestions":"123"},{"ruleId":"116","severity":1,"message":"124","line":1,"column":17,"nodeType":"118","messageId":"119","endLine":1,"endColumn":26},{"ruleId":"125","severity":1,"message":"126","line":25,"column":41,"nodeType":"127","messageId":"128","endLine":25,"endColumn":43},{"ruleId":"116","severity":1,"message":"129","line":1,"column":17,"nodeType":"118","messageId":"119","endLine":1,"endColumn":25},{"ruleId":"116","severity":1,"message":"130","line":1,"column":37,"nodeType":"118","messageId":"119","endLine":1,"endColumn":47},{"ruleId":"116","severity":1,"message":"131","line":3,"c